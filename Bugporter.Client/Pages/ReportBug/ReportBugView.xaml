<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="Bugporter.Client.Pages.ReportBug.ReportBugView"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    Title="ReportBugView">
    <VerticalStackLayout>
        <Label
            HorizontalOptions="Center"
            Text="{Binding Value}"
            VerticalOptions="Center" />
        <Entry Placeholder="Enter a value" Text="{Binding Value}" />
    </VerticalStackLayout>
</ContentPage>